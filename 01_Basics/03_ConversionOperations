// Convert to Number

let score = undefined;

let toNumber = Number(score);
console.log(toNumber);
console.log(typeof(toNumber));

// Convert to String

let grade  = NaN;

let toStr = String (grade);

console.log(toStr);
console.log(typeof toStr);


// COnver to Boolean

let isLogged = "";

let toBoolean = Boolean (isLogged);

console.log(toBoolean);
console.log(typeof toBoolean);






